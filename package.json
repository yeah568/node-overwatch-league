{
  "name": "node-overwatch-league",
  "version": "0.0.6",
  "description": "A Node wrapper for the Overwatch League API.",
  "homepage": "https://github.com/yeah568/node-overwatch-league",
  "author": {
    "name": "James Liu",
    "email": "james@jamesliu.ca",
    "url": "jamesliu.ca"
  },
  "files": ["lib"],
  "main": "lib/index.js",
  "keywords": ["overwatch"],
  "devDependencies": {
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "https://github.com/yeah568/node-overwatch-league.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.1.1",
    "whatwg-url": "^6.4.0"
  }
}
